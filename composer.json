{
  "name": "richardhj/contao-ferienpass",
  "description": "FERIENPASS extension for Contao Open Source CMS built on the MetaModels extension",
  "type": "contao-module",
  "homepage": "https://ferienpass.online/",
  "authors": [
    {
      "name": "Richard Henkenjohann",
      "email": "richard@ferienpass.online",
      "homepage": "http://henkenjohann.me",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "richard@ferienpass.online"
  },
  "require": {
    "php": "^5.4 || ^7.0",
    "codefog/contao-haste": "~4.11",
      "contao-community-alliance/dependency-container": "~1.8",
      "contao-community-alliance/event-dispatcher": "~1.3",
    "contao-community-alliance/meta-palettes": "~1.9",
    "eluceo/ical": "~0.8",
      "friends-of-contao/contao-associategroups": "^1.3",
    "league/flysystem-dropbox": "~1.0",
    "leounglaub/contao-simple-ajax": "3.2.x-dev",
    "metamodels/bundle_all": "~2.0.0",
    "netzmacht/contao-flysystem": "~1.0",
    "pixelfear/oauth2-dropbox": "dev-master",
    "richardhj/contao-member_logo": "*",
    "richardhj/contao-model-events": "^1.0",
    "richardhj/contao-upload_preview_field": "*",
      "richardhj/dc-general-single-model": "*",
    "terminal42/contao-conditionalselectmenu": "~3.0",
    "terminal42/contao-tablelookupwizard": "^3.2",
    "terminal42/notification_center": "~1.3"
  },
  "autoload": {
    "psr-0": {
      "Ferienpass\\": "src/",
      "MetaModels\\": "src/",
      "League\\": "src/"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "assets": "assets/ferienpass/backend",
        "module": "system/modules/ferienpass"
      }
    }
  }
}
