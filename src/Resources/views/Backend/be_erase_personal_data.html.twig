{% extends "@MetaModelsCore/Backend/be_base.html.twig" %}

{% block headline %}
    {{ headline }}
{% endblock %}

{% block error %}
    {%- if error is defined -%}
        {{- error -}}
    {%- endif -%}
{% endblock %}

{% block main %}
    <div class="tl_listing_container">
        <div class="tl_tbox block">
            <h2>{{ sub_headline }}</h2>

            <div class="right">
                {{ form | raw }}
            </div>
        </div>
    </div>
{% endblock %}
